using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BusinessAccessLayer.BO
{
    public class ExpenseEntryBO
    {
        private string _category_Name;
        private int _category_ID;
        private string _expense_Description;
        private string _expense_Frequency;

        public ExpenseEntryBO()
        {
            _category_Name = string.Empty;
            _expense_Description = string.Empty;
            _expense_Frequency = string.Empty;
            _category_ID = 0;
        }

        public string Category_Name
        {
            get { return _category_Name; }
            set { _category_Name = value; }
        }

        public string Expense_Description
        {
            get { return _expense_Description; }
            set { _expense_Description = value; }
        }

        public string Expense_Frequency
        {
            get { return _expense_Frequency; }
            set { _expense_Frequency = value; }
        }

        public int Category_ID
        {
            get { return _category_ID; }
            set { _category_ID = value; }
        }
    }
}
