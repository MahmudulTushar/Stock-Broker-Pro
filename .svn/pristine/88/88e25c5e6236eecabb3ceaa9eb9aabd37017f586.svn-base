using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BusinessAccessLayer.BO
{
    public class DefineLiabilitiesBO
    {
        private string _Lib_Name;
        private string _Deduction_Period;
        private double _Payment_Amount;
        private string _Remarks;
        private string _Status;
        private DateTime _Entry_Date;
        private string _Entry_By;
        private DateTime _Update_Date;
        private string _Update_By;

        public DefineLiabilitiesBO()
        {
            _Lib_Name = string.Empty;
            _Deduction_Period = string.Empty;
            _Payment_Amount = 0;
            _Remarks = string.Empty;
            _Status = string.Empty;
            _Entry_Date = DateTime.MinValue;
            _Entry_By = string.Empty;
            _Update_Date = DateTime.MinValue;
            _Update_By = string.Empty;
        }

        public string Lib_Name
        {
            get { return _Lib_Name; }
            set { _Lib_Name = value; }
        }
        public string Deduction_Period
        {
            get { return _Deduction_Period; }
            set { _Deduction_Period = value; }
        }
        public double Payment_Amount
        {
            get { return _Payment_Amount; }
            set { _Payment_Amount = value; }
        }
        public string Remarks
        {
            get { return _Remarks; }
            set { _Remarks = value; }
        }
        public string Status
        {
            get { return _Status; }
            set { _Status = value; }
        }
        public DateTime Entry_Date
        {
            get { return _Entry_Date; }
            set { _Entry_Date = value; }
        }
        public string Entry_By
        {
            get { return _Entry_By; }
            set { _Entry_By = value; }
        }
        public DateTime Update_Date
        {
            get { return _Update_Date; }
            set { _Update_Date = value; }
        }
        public string Update_By
        {
            get { return _Update_By; }
            set { _Update_By = value; }
        }
    }
}
