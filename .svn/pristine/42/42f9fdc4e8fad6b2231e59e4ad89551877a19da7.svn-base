using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace BusinessAccessLayer.BO
{
    public class OpexDailyBO
    {
        private int _expenseId;
        private int _purposeID;
        private int _branchID;
        private float _amount;
        private string _voucherNo;
        private byte[] _voucherImage;
        private DateTime _expenseDate;
        private string _remarks;
        private DateTime _monthOfExpense;
        private string _purpose;
        public OpexDailyBO()
        {
            _amount =0;
            _voucherNo = "";
            _voucherImage = new byte[0];
            _remarks = "";
        }

        public int PurposeId
        {
            get { return _purposeID; }
            set { _purposeID = value; }
        }

        public int BranchId
        {
            get { return _branchID; }
            set { _branchID = value; }
        }

        public float Amount
        {
            get { return _amount; }
            set { _amount = value; }
        }

        public string VoucherNo
        {
            get { return _voucherNo; }
            set { _voucherNo = value; }
        }

        public byte[] VoucherImage
        {
            get { return _voucherImage; }
            set { _voucherImage = value; }
        }

        public DateTime ExpenseDate
        {
            get { return _expenseDate; }
            set { _expenseDate = value; }
        }

        public int ExpenseId
        {
            get { return _expenseId; }
            set { _expenseId = value; }
        }

        public string Remarks
        {
            get { return _remarks; }
            set { _remarks = value; }
        }

        public DateTime MonthOfExpense
        {
            get { return _monthOfExpense; }
            set { _monthOfExpense = value; }
        }

        private string _expenseType;
        public String ExpenseType
        {
            get { return _expenseType; }
            set { _expenseType = value; }
        }

        public string Purpose
        {
            get { return _purpose; }
            set { _purpose = value; }
        }
    }
}
